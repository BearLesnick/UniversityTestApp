<?xml version="1.0" encoding="utf-8"?>
<menu xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <item
        android:icon="@android:drawable/ic_dialog_email"
        android:onClick="onMenuContactsItemClick"
        android:orderInCategory="1"
        android:title="@string/contacts_menu_item"
        app:showAsAction="ifRoom" />
    <item
        android:icon="@android:drawable/ic_menu_upload"
        android:onClick="onOpenDraftsList"
        android:orderInCategory="2"
        android:title="@string/drafts"
        app:showAsAction="ifRoom" />
    <item
        android:icon="@android:drawable/ic_menu_save"
        android:onClick="onAddToDraft"
        android:orderInCategory="3"
        android:title="@string/savedraft"
        app:showAsAction="ifRoom" />
    <item
        android:icon="@android:drawable/ic_menu_add"
        android:title="@string/random_content"
        android:onClick="onRandomContent"/>
    <item
        android:id="@+id/web_view_button"
        android:icon="@android:drawable/ic_menu_search"
        android:onClick="startWebActivity"
        android:title="Browser"
        app:actionProviderClass="android.support.v7.app.MediaRouteActionProvider"/>
    <item
        android:id="@+id/htmp_parser_item"
        android:icon="@android:drawable/ic_menu_search"
        android:onClick="startHtmlActivity"
        android:title="Title Analizer"
        app:actionProviderClass="android.support.v7.app.MediaRouteActionProvider" />

    <item
        android:id="@+id/navigation"
        android:icon="@android:drawable/ic_menu_search"
        android:onClick="startSensorActivity"
        android:title="Sensor"
        app:actionProviderClass="android.support.v7.app.MediaRouteActionProvider" />
</menu>